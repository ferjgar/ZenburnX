<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">
<fontscolors>
    <fontcolor name="error" underline="ffff3333"/>
    <fontcolor foreColor="ff8aa6c1" name="char"/>
    <fontcolor foreColor="ffdfc47d" name="keyword">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="whitespace"/>
    <fontcolor default="keyword" name="markup-element"/>
    <fontcolor foreColor="ff8cd0d3" name="number"/>
    <fontcolor foreColor="ffdfc47d" name="operator"/>
    <fontcolor name="url"/>
    <fontcolor default="identifier" name="field"/>
    <fontcolor bgColor="ff3f3f3f" foreColor="white" name="default">
        <font name="Consolas" size="14" style="plain"/>
    </fontcolor>
    <fontcolor foreColor="ffcc9393" name="string"/>
    <fontcolor default="default" foreColor="magenta" name="entity-reference"/>
    <fontcolor default="string" name="markup-attribute-value"/>
    <fontcolor default="identifier" foreColor="white" name="method"/>
    <fontcolor foreColor="ff6a9f7f" name="comment">
        <font style="bold+italic"/>
    </fontcolor>
    <fontcolor foreColor="ffdcdccc" name="identifier"/>
    <fontcolor name="warning" waveUnderlined="yellow"/>
    <fontcolor foreColor="ff9f9d6d" name="separator"/>
    <fontcolor default="field" foreColor="white" name="markup-attribute"/>
</fontscolors>
